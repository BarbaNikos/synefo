set multiplot layout 3,1
set xdata time
set timefmt "%s"
set xlabel "Time progress"
set ylabel "Input rate (tuples/sec)"
plot "input-rate-5.dat" using 1:2 with lines
set ylabel "Response time (nsec)"
plot "round-trip-5.dat" using 1:3 with lines
set ylabel "State (bytes)"
plot "state-5.dat" using 1:2 with lines
unset multiplot